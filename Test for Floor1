! Basic Config
enable
configure terminal
hostname Acc-Floor1
enable secret Windows1
line console 0
 password Windows1
 login
 logging synchronous
line vty 0 15
 password Windows1
 login
 transport input ssh
 logging synchronous
exit
banner motd #Authorized Access Only!#
clock timezone EST -5
sdm prefer dual-ipv4-and-ipv6 default

! VLANs
vlan 105
 name PharmaPlus
vlan 115
 name DynaLabs
vlan 125
 name Reception
vlan 135
 name Security
vlan 145
 name Facilities
vlan 155
 name Visitor1
vlan 720
 name IT

! VLAN Interfaces
interface Vlan105
 description VLAN for PharmaPlus
 ip address 10.150.8.1 255.255.255.0
 ipv6 address 2001:bbfc:88ef:8::1/64
 no shutdown

interface Vlan115
 description VLAN for DynaLabs
 ip address 10.150.12.1 255.255.255.0
 ipv6 address 2001:bbfc:88ef:12::1/64
 no shutdown

interface Vlan125
 description VLAN for Reception
 ip address 10.150.16.1 255.255.255.0
 ipv6 address 2001:bbfc:88ef:16::1/64
 no shutdown

interface Vlan135
 description VLAN for Security
 ip address 10.150.20.1 255.255.255.0
 ipv6 address 2001:bbfc:88ef:20::1/64
 no shutdown

interface Vlan145
 description VLAN for Facilities
 ip address 10.150.24.1 255.255.255.0
 ipv6 address 2001:bbfc:88ef:24::1/64
 no shutdown

interface Vlan155
 description VLAN for Visitor1
 ip address 10.150.28.1 255.255.255.0
 ipv6 address 2001:bbfc:88ef:28::1/64
 no shutdown

interface Vlan720
 description VLAN for IT
 ip address 10.150.120.1 255.255.255.0
 ipv6 address 2001:bbfc:88ef:120::1/64
 no shutdown

! Access Ports Configuration
interface GigabitEthernet0/1
 description Access port for PharmaPlus
 switchport mode access
 switchport access vlan 105
 spanning-tree portfast
 spanning-tree bpduguard enable
 no shutdown

interface GigabitEthernet0/2
 description Access port for DynaLabs
 switchport mode access
 switchport access vlan 115
 spanning-tree portfast
 spanning-tree bpduguard enable
 no shutdown

interface GigabitEthernet0/3
 description Access port for Reception
 switchport mode access
 switchport access vlan 125
 spanning-tree portfast
 spanning-tree bpduguard enable
 no shutdown

interface GigabitEthernet0/4
 description Access port for Security
 switchport mode access
 switchport access vlan 135
 spanning-tree portfast
 spanning-tree bpduguard enable
 no shutdown

interface GigabitEthernet0/5
 description Access port for Facilities
 switchport mode access
 switchport access vlan 145
 spanning-tree portfast
 spanning-tree bpduguard enable
 no shutdown

interface GigabitEthernet0/6
 description Access port for Visitor1
 switchport mode access
 switchport access vlan 155
 spanning-tree portfast
 spanning-tree bpduguard enable
 no shutdown

interface GigabitEthernet0/7
 description Access port for IT
 switchport mode access
 switchport access vlan 720
 spanning-tree portfast
 spanning-tree bpduguard enable
 no shutdown

! Configure trunk ports
interface GigabitEthernet0/0
 description Trunk link to SpringCore1
 switchport mode trunk
 switchport trunk allowed vlan all
 no shutdown

interface GigabitEthernet0/8
 description Trunk link to SpringCore2
 switchport mode trunk
 switchport trunk allowed vlan all
 no shutdown

! VTP Configuration
vtp mode transparent

! Save Config
end
write memory
